<%- include ../publicView/res/style %>
<div class="z-container">
	<!--
    	时间：2016-08-04
    	描述：头部
    -->
	<%- include ../publicView/headAndTail/header %>
	<!--
    	
    	时间：2016-08-04
    	描述：菜单和内容
    -->	
    <div class="container z-clearFix z-centerInfo-box z-body">
    	
    	
    	<div class="row">
    	
        	<!--
            	
            	时间：2016-08-05
            	描述：侧栏
            -->
    		<%- include ../publicView/sidebar/sidebar %>
			
    		<!--
            	
            	时间：2016-08-05
            	描述：内容区块
            -->
    		<div class="z-rightContainer-box">
				
				<!--
                	
                	时间：2016-08-24
                	描述：用户管理头部
                -->
				<div class="z-rightContent-tittle">
    					
    					<div class="z-conTittle-tab">
    						
    						<ul class="z-tabContent-ul z-clearFix">
    							
							    <li>
							    	<a href="">表单</a>
							    </li>
							    
							</ul>
							
						</div>
						
    				</div>
				
				<!--
                	时间：2016-09-10
                	描述：搜索控件
                -->
				<div class="z-searchUser-box">
					
					<form>
						<!--
                        	
                        	时间：2016-08-24
                        	描述：关键词
                        -->
						<div class="z-controlInput-box z-clearFix">
    						
    						<div class="z-searchInput-list">
    							
    							<span class="tittle">关键词</span>
        						
        						<p class="input">
        							
        							<span>
        								
        								<input type="text" class="form-control"/>
        								
        							</span>
        							
        						</p>
    							
    						</div>
    						
    					</div>
    					
					</form>
					
				</div>
				
				<!--
                	
                	时间：2016-08-25
                	描述：文章管理
                -->
				<div class="z-tabPage-box" style="padding-bottom:30px;">
					
					<div class="tableInfo">
						<table class="table">
					      <thead>
					        <tr class="active"> 
					          <th>编辑者</th>
					          <th>审核状态</th>
					          <th>标题</th>
					          <th>时间</th>
					          <th>操作</th>
					        </tr>
					      </thead>
					      <tbody>
		      			    <% for(var i=0; i<list.length; i++) { %>
						        <tr>
						        	<td><%=list[i].userName %></td>
						        	<% if (list[i].examine=="0") { %>
						        		<td>未审核</td>
						        	<%}else{%>
						        		<td>已审核</td>
						        	<%}%>
						        	<td><%=list[i].title %></td>
						        	<td><%=list[i].timer %></td>
						        	<td>
						        		<a href="/cgarticle?id=<%=list[i].artid %>">修改</a>
						        		<% if (list[i].examine=="0") { %>
						        			<a href="javascript:void(0)" onclick="examineArticFun('<%=list[i].artid %>')">审核</a>
						        		<%}%>
						        		<a href="javascript:void(0)" onclick="deleteArticFun('<%=list[i].artid %>')">删除</a>
						        		<a href="/articinfo?id=<%=list[i].artid %>">查看</a>
						        	</td>
						        </tr>
						    <% } %>
					      </tbody>
					    </table>
						
					</div>
					
				</div>
				
    		</div>
    		
    	</div>	
    	
    </div>
	<!--
    	
    	时间：2016-08-13
    	描述：脚步
    -->
	<%- include ../publicView/headAndTail/footer %>
	
</div>
<!--
	时间：2017-02-21
	描述：资源文件引入
-->
<%- include ../publicView/res/script %>